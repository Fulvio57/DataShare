    -<contexts>
      <context attribute="Normal Text" lineEndContext="#stay" name="Normal">
        <StringDetect attribute="Assembler Words" context="#stay" String="asm" insensitive="true" beginRegion="sAsmRegion"/>
        <StringDetect attribute="Assembler Words" context="#stay" String="endasm" insensitive="true" endRegion="sAsmRegion"/>
        <!-- define, elif, else, endif, error, if, ifdef, ifndef, line, pragma, undef, warning -->
        <RegExpr attribute="Preprocessor" context="Preprocessor" String="\$\s*if(?:def|ndef)?(?=\s+\S)" insensitive="true" beginRegion="PP" firstNonSpace="true" />
        <RegExpr attribute="Preprocessor" context="Preprocessor" String="\$\s*endif" insensitive="true" endRegion="PP" firstNonSpace="true" />
        <RegExpr attribute="Preprocessor" context="Define" String="\$\s*define.*((?=\\))" insensitive="true" firstNonSpace="true" />
        <keyword attribute="Preprocessor" context="#stay" String="Preprocessor"/>
        <LineContinue attribute="Keywords" context="#stay" char="_"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(while)([\s]|$)" insensitive="true" beginRegion="WhileRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(wend)([\s]|$)" insensitive="true" endRegion="WhileRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(do)([\s]|$)" insensitive="true" beginRegion="DoRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(loop)([\s]|$)" insensitive="true" endRegion="DoRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(repeat)([\s]|$)" insensitive="true" beginRegion="repeatRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(until)([\s]|$)" insensitive="true" endRegion="repeatRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(select)([\s]|$)" insensitive="true" beginRegion="SelectRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="(\b(end select)|\b(endselect))([\s]|$)" insensitive="true" endRegion="SelectRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(for)([\s]|$)" insensitive="true" beginRegion="ForRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(next)([\s]|$)" insensitive="true" endRegion="ForRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(sub)([.\s]|$)" insensitive="true" beginRegion="sProcedureRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(endsub)([\s]|$)" insensitive="true" endRegion="sProcedureRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(proc)([.\s]|$)" insensitive="true" beginRegion="sProcedureRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(endproc)([\s]|$)" insensitive="true" endRegion="sProcedureRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="\b(if)([\s]|$)" insensitive="true" beginRegion="IfRegion"/>
        <RegExpr attribute="Keywords" context="#stay" String="(\b(end if)|\b(endif)|(\bthen \b\w+[^;']))([\s]|$)" insensitive="true" endRegion="IfRegion"/>
<!--
        <RegExpr attribute="Keywords" context="#stay" String="(\bthen [^\s].+)|(\b(end if|else|elseif|else if)|(endif)$)" insensitive="true" endRegion="IfRegion"/>
        -->
<!-- labels -->
        <RegExpr attribute="Label" context="#pop" String="^(_*[A-Za-z0-9])+?\:(\s+|$)"/>
<!-- labels -->
        <keyword attribute="Keywords" context="#stay" String="Keywords"/>
        <keyword attribute="Declares" context="#stay" String="Declares"/>
        <keyword attribute="Macro" context="#stay" String="Macro"/>
        <keyword attribute="Important" context="#stay" String="Important"/>
        <keyword attribute="Reserved Words" context="#stay" String="Reserved Words"/>
        <keyword attribute="Assembler Words" context="#stay" String="Assembler Words"/>
        <RegExpr attribute="Constant" context="#stay" String="\#+[a-zA-Z_\x7f-\xff][a-zA-Z0-9_\x7f-\xff]*"/>
<!-- Hex and Binary notation -->
        <RegExpr attribute="Number" context="#stay"  insensitive="true" String="\$[a-f0-9]+"/>
        <RegExpr attribute="Number" context="#stay"  insensitive="true" String="0[bB][0-9]+"/>
        <HlCHex attribute="Number" context="#stay"/>
<!-- Regular number notation -->
        <Float attribute="Number" context="#stay"/>
        <Int attribute="Number" context="#stay"/>
        <Detect2Chars attribute="Comment" context="#pop!Commentar 2" char="(" char1="*" beginRegion="Comment" />
        <DetectChar attribute="String" context="String" char="&quot;" />
        <DetectChar attribute="Number" context="#stay" char="%" />
        <IncludeRules context="Number" />
        <DetectChar attribute="Comment" context="Comment1" char="'"/>
        <DetectChar attribute="Comment" context="Comment1" char=";"/>
      </context>
      <context attribute="String" lineEndContext="#pop" name="String">
        <DetectChar attribute="String" context="#pop" char="&quot;"/>
      </context>
      <context attribute="Comment" lineEndContext="#stay" name="Commentar 2">
        <Detect2Chars attribute="Comment" context="#pop" char="*" char1=")" endRegion="Comment"/>
        <IncludeRules context="##Comments" />
      </context>
      <context attribute="Comment" lineEndContext="#pop" name="Comment1">
        <keyword attribute="Number" context="#stay" String="attention" />
      </context>
    </contexts>
